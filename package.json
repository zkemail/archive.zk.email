{
	"name": "archive.prove.email",
	"version": "0.1.0",
	"license": "MIT",
	"private": true,
	"engines": {
		"node": ">=v22.2.0"
	},
	"scripts": {
		"dev": "next dev",
		"build": "prisma generate --generator client_js && prisma migrate deploy && next build",
		"start": "next start",
		"preinstall": "npx only-allow pnpm",
		"check-types": "tsc --noEmit",
		"lint": "next lint",
		"test": "vitest run",
		"start-cron": "tsx src/util/cron.ts",
		"infra:init": "cd terraform && terraform init",
		"infra:plan": "cd terraform && terraform plan -var-file=./terraform.tfvars",
		"infra:apply": "cd terraform && terraform apply -var-file=./terraform.tfvars -auto-approve",
		"infra:destroy": "cd terraform && terraform destroy -var-file=./terraform.tfvars -auto-approve",
		"deploy:infra": "pnpm infra:init && pnpm infra:apply",
		"destroy:infra": "pnpm infra:destroy"
	},
	"prisma": {
		"seed": "tsx prisma/seed.ts"
	},
	"dependencies": {
		"@emotion/react": "^11.11.4",
		"@emotion/styled": "^11.11.5",
		"@google-cloud/tasks": "^5.1.0",
		"@mui/material": "^5.15.19",
		"@prisma/client": "5.17.0",
		"@types/node-forge": "^1.3.11",
		"@witnessco/client": "^0.4.2",
		"@zk-email/helpers": "^6.4.2",
		"axios": "^1.7.2",
		"chalk": "^5.4.1",
		"dkimpy": "^3.0.1",
		"googleapis": "^137.1.0",
		"lodash": "^4.17.21",
		"lru-cache": "^11.2.1",
		"moment": "^2.30.1",
		"next": "14.2.3",
		"next-auth": "^4.24.7",
		"node-cron": "^3.0.3",
		"node-forge": "^1.3.1",
		"rate-limiter-flexible": "^5.0.3",
		"react": "^18.3.1",
		"react-datepicker": "^8.4.0",
		"react-dom": "^18.3.1",
		"react-intersection-observer": "^9.10.2",
		"viem": "^2.13.1",
		"zod": "^3.23.8"
	},
	"//": "prisma 5.11.0 needed for Prisma Client Python",
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/lodash": "^4.17.10",
		"@types/node": "^20.12.12",
		"@types/node-cron": "^3.0.11",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"@types/swagger-ui-react": "^4.18.3",
		"autoprefixer": "^10.4.19",
		"dkim": "^0.8.0",
		"postcss": "^8.4.38",
		"prisma": "5.17.0",
		"swagger-ui-react": "^5.17.14",
		"tsx": "^4.11.0",
		"typescript": "^5.4.5",
		"vitest": "^1.6.0"
	}
}
